    "custom_liquid_VkJypp": {
      "type": "custom-liquid",
      "name": "Custom Liquid",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "liquid": "<script>\n(function(){\n  const ALL_VIDEO_ITEM = '.product-gallery__media[data-media-type=\"video\"], [data-media-type=\"video\"], video-media';\n  const RETRY_MS = 600;\n  const MAX_RETRIES = 40;\n\n  function extractSrc(container) {\n    if (!container) return null;\n    try {\n      // direct <video>\n      const vid = container.querySelector('video');\n      if (vid) {\n        const srcEl = vid.querySelector('source') || vid;\n        const s = srcEl.src || srcEl.getAttribute('src');\n        if (s) return s.toString();\n      }\n      // <source> tag\n      const srcNode = container.querySelector('source');\n      if (srcNode) {\n        const s2 = srcNode.src || srcNode.getAttribute('src');\n        if (s2) return s2.toString();\n      }\n      // shadow DOM fallback\n      if (container.shadowRoot) {\n        const shadowVid = container.shadowRoot.querySelector('video');\n        if (shadowVid) {\n          const srcEl = shadowVid.querySelector('source') || shadowVid;\n          const s3 = srcEl && (srcEl.src || srcEl.getAttribute('src'));\n          if (s3) return s3.toString();\n        }\n      }\n    } catch(e){}\n    return null;\n  }\n\n  function buildNativeVideo(src, poster) {\n    const v = document.createElement('video');\n    v.src = src;\n    v.autoplay = true;\n    v.muted = true;\n    v.loop = true;\n    v.playsInline = true;\n    v.preload = 'auto';\n    v.setAttribute('muted','');\n    v.setAttribute('playsinline','');\n    v.setAttribute('autoplay','');\n    v.setAttribute('loop','');\n    if (poster) v.poster = poster;\n    v.className = 'forced-autoplay-native';\n    v.style.width = '100%';\n    v.style.height = '100%';\n    v.style.objectFit = 'cover';\n    v.style.display = 'block';\n    return v;\n  }\n\n  function replaceWithNative(container, src, poster) {\n    if (!container || !src) return false;\n    try {\n      if (!container.dataset.originalHtml) {\n        container.dataset.originalHtml = container.innerHTML;\n      }\n      const existing = container.querySelector('.forced-autoplay-native');\n      if (existing) return true;\n\n      const wrapper = document.createElement('div');\n      wrapper.style.width = '100%';\n      wrapper.style.height = '100%';\n      wrapper.style.position = 'relative';\n      wrapper.className = 'forced-autoplay-wrapper';\n\n      const nativeVideo = buildNativeVideo(src, poster);\n      nativeVideo.style.pointerEvents = 'auto';\n\n      container.innerHTML = '';\n      wrapper.appendChild(nativeVideo);\n      container.appendChild(wrapper);\n\n      setTimeout(() => {\n        try { nativeVideo.play().catch(()=>{}); } catch(e){}\n      }, 50);\n\n      return true;\n    } catch(e){ return false; }\n  }\n\n  let attempts = 0;\n  function autoplayFirstVideo() {\n    attempts++;\n    const allVideos = document.querySelectorAll(ALL_VIDEO_ITEM);\n    if (!allVideos.length) {\n      if (attempts < MAX_RETRIES) setTimeout(autoplayFirstVideo, RETRY_MS);\n      return;\n    }\n\n    // Only target the first video element in the gallery\n    const firstVideoContainer = allVideos[0];\n    const src = extractSrc(firstVideoContainer);\n\n    let poster = null;\n    try {\n      const img = firstVideoContainer.querySelector('img');\n      if (img && (img.src || img.getAttribute('src'))) poster = img.src || img.getAttribute('src');\n    } catch(e){}\n\n    if (src) {\n      replaceWithNative(firstVideoContainer, src, poster);\n    } else if (attempts < MAX_RETRIES) {\n      setTimeout(autoplayFirstVideo, RETRY_MS);\n    }\n  }\n\n  window.addEventListener('load', () => setTimeout(autoplayFirstVideo, 300));\n  document.addEventListener('shopify:section:load', () => setTimeout(autoplayFirstVideo, 300));\n  document.addEventListener('DOMContentLoaded', () => setTimeout(autoplayFirstVideo, 300));\n\n})();\n</script>",
        "background": "",
